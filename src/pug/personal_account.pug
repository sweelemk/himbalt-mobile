extends _layout

block title
	title Personal account
block content
	.root_wrapper.load-dom
		i.overlay
		.root_frame
			.root_frame-content
				.pages
					.section-scroll-container
						.section-scroll.js-ajx(data-pages="false")
							article.container-content
								section.intro-head
									.logo-container
										- var Index = true
										if(Index)
											.logo
										else
											a(href="/" class="js-link logo")
									.ui-menu
										span.ui-menu__content
											i.ui-menu-line.ui-menu-line-1
											i.ui-menu-line.ui-menu-line-2
											i.ui-menu-line.ui-menu-line-3
									.breadcumbs
										.breadcumbs-inner
											.breadcrumbs-content
												.breadcrumbs-item
													a(href="/" class="js-link")
														span Home
												.breadcrumbs-item
													a(href="/" class="js-link")
														span Personal account
									.intro-head-container
										h1.h1 
											span Personal account
								section.section-anchors.js-section(data-inview-class="push-it")
									.tab-container
										.tab-navigation
											.anchor-container
												ul
													mixin anchors(name, link, bool)
														if bool 
															li.achors-item.active
																a.anchor-trigger(href=link)
																	span #{name}
														else 
															li.achors-item
																a.anchor-trigger(href=link)
																	span #{name}
													+anchors("Documents for download", "personal_account.html", true)
													+anchors("Personal information", "personal_account_info.html", false)
